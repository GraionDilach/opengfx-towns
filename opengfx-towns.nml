//secondary switch function, for house on 'inner corners' diagonally next to road
switch (FEAT_HOUSES, SELF, switch_house_orientation_roadinnercorner, [
	1 * (nearby_tile_class( 1,  1) == TILE_CLASS_ROAD) + 1 * (nearby_tile_class( 1,  1) == TILE_CLASS_STATION) +
	2 * (nearby_tile_class( 1, -1) == TILE_CLASS_ROAD) + 2 * (nearby_tile_class( 1, -1) == TILE_CLASS_STATION) +
	4 * (nearby_tile_class(-1, -1) == TILE_CLASS_ROAD) + 4 * (nearby_tile_class(-1, -1) == TILE_CLASS_STATION) +
	8 * (nearby_tile_class(-1,  1) == TILE_CLASS_ROAD) + 8 * (nearby_tile_class(-1,  1) == TILE_CLASS_STATION)
]) {
	//One neighbouring road
	1: return 5;
	2: return 6;
	4: return 7;
	8: return 8;
    return 9;
}

//primary switch function, for house next to road
switch (FEAT_HOUSES, SELF, switch_house_orientation_8road, [
	1 * (nearby_tile_class( 0,  1) == TILE_CLASS_ROAD) + 1 * (nearby_tile_class( 0,  1) == TILE_CLASS_STATION) +
	2 * (nearby_tile_class( 1,  0) == TILE_CLASS_ROAD) + 2 * (nearby_tile_class( 1,  0) == TILE_CLASS_STATION) +
	4 * (nearby_tile_class( 0, -1) == TILE_CLASS_ROAD) + 4 * (nearby_tile_class( 0, -1) == TILE_CLASS_STATION) +
	8 * (nearby_tile_class(-1,  0) == TILE_CLASS_ROAD) + 8 * (nearby_tile_class(-1,  0) == TILE_CLASS_STATION)
]) {
	//One neighbouring road
	1: return 1;
	2: return 2;
	4: return 3;
	8: return 4;
	//Two neighbouring roads
	//... adjacent
	3: return 1;
	6: return 2;
	12: return 3;
	9: return 4;
	//... opposite
	5: return 1;
	10: return 2;
	//Three neighbouring roads
	7: return 2;
	14: return 3;
	13: return 4;
	11: return 1;
	//Four neighbouring roads
	15: return 9;
	//No orthogonal adjacent road, so check diagonal adjacency
	switch_house_orientation_roadinnercorner;
}

