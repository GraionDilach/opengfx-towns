spritelayout sprite_ukts_church_small_1a {
    ground {
        sprite: construction_state == 0 ? GROUNDSPRITE_CLEARED
            : terrain_type == TILETYPE_SNOW ? GROUNDSPRITE_SNOW
                : terrain_type == TILETYPE_DESERT ? GROUNDSPRITE_DESERT
                    : GROUNDSPRITE_NORMAL;
    }
    childsprite {
        sprite: ukts_church_small_1_ground_snow;
        hide_sprite: terrain_type != TILETYPE_SNOW;
        always_draw: 1;
    }
    childsprite {
        sprite: ukts_church_small_1_ground_arctic;
        hide_sprite: terrain_type != TILETYPE_DESERT;
        always_draw: 1;
    }
    childsprite {
        sprite: ukts_church_small_1_ground_temperate;
        hide_sprite: terrain_type == TILETYPE_SNOW || terrain_type == TILETYPE_DESERT
            || !(landscape == 1 || (landscape == 0 && climate == CLIMATE_TEMPERATE));
        always_draw: 1;
    }
    childsprite {
        sprite: ukts_church_small_1_ground_arctic;
        hide_sprite: terrain_type == TILETYPE_SNOW || terrain_type == TILETYPE_DESERT
            || !(landscape == 2 || (landscape == 0 && climate == CLIMATE_ARCTIC));
        always_draw: 1;
    }
    childsprite {
        sprite: ukts_church_small_1_ground_tropic;
        hide_sprite: terrain_type == TILETYPE_SNOW || terrain_type == TILETYPE_DESERT
            || !(landscape == 3 || (landscape == 0 && climate == CLIMATE_TROPIC));
        always_draw: 1;
    }
    building {
        sprite: ukts_church_small_1a_building;
        hide_sprite: terrain_type == TILETYPE_SNOW;
    }
    building {
        sprite: ukts_church_small_1a_building_snow;
        hide_sprite: terrain_type != TILETYPE_SNOW;
    }
}

