    error(FATAL, REQUIRES_OPENTTD, "15.0");
}


grf {
    grfid: "zgy\02";
    name: string(STR_GRF_NAME);
    desc: string(STR_GRF_DESCRIPTION);
    version: 0001;
    min_compatible_version: 0;
    url: string(STR_GRF_URL);

    param 0 {
        disable_base_houses {
            type: bool;
            bit: 0;
            name: string(STR_PARAM_NAME_DISABLE_BASE_HOUSES);
            def_value: 0;
        }

        enable_temperate {
            type: bool;
            bit: 1;
            name: string(STR_PARAM_NAME_ENABLE_TEMPERATE);
            def_value: 1;
        }

        enable_arctic {
            type: bool;
            bit: 2;
            name: string(STR_PARAM_NAME_ENABLE_ARCTIC);
            def_value: 1;
        }

        enable_tropic {
            type: bool;
            bit: 3;
            name: string(STR_PARAM_NAME_ENABLE_TROPIC);
            def_value: 1;
        }

        enable_uktownset {
            type: bool;
            bit: 4;
            name: string(STR_PARAM_NAME_ENABLE_UK_TOWN_SET);
            def_value: 1;
        }

        // dedicate bits incase I ever get there to retexture zbase-8bpp into OpenGFX
        // would make sense to substitute back the vanilla building slots which I'll drop with UKTownSet

        // zbase temperate bit 5
        // zbase arctic bit 6
        // zbase tropic bit 7

        enable_toyland {
            type: bool;
            bit: 8;
            name: string(STR_PARAM_NAME_ENABLE_TOYLAND);
            desc: string(STR_PARAM_DESC_ENABLE_TOYLAND);
            def_value: 0;
        }

        enable_32bpp_arctic_hotel {
            type: bool;
            bit: 9;
            name: string(STR_PARAM_NAME_ENABLE_32BPP_ARCTIC_HOTEL);
            def_value: 0;
        }

        enable_bank {
            type: bool;
            bit: 10;
            name: string(STR_PARAM_NAME_ENABLE_BANK);
            desc: string(STR_PARAM_DESC_ENABLE_BANK);
            def_value: 0;
        }

        enable_industries {
            type: bool;
            bit: 11;
            name: string(STR_PARAM_NAME_ENABLE_INDUSTRIES);
            desc: string(STR_PARAM_DESC_ENABLE_INDUSTRIES);
            def_value: 0;
        }

        enable_hq {
            type: bool;
            bit: 12;
            name: string(STR_PARAM_NAME_ENABLE_HQ);
            desc: string(STR_PARAM_DESC_ENABLE_HQ);
            def_value: 0;
        }

        enable_misc {
            type: bool;
            bit: 13;
            name: string(STR_PARAM_NAME_ENABLE_MISC);
            desc: string(STR_PARAM_DESC_ENABLE_MISC);
            def_value: 0;
        }

        protect_multicell {
            type: bool;
            bit: 15;
            name: string(STR_PARAM_NAME_PROTECT_MULTICELL);
            desc: string(STR_PARAM_DESC_PROTECT_MULTICELL);
            def_value: 1;
        }
    }

    param 1 {
        landscape {
            type: int;
            name: string(STR_PARAM_NAME_LANDSCAPE);
            desc: string(STR_PARAM_DESC_LANDSCAPE);
            min_value: 0;
            max_value: 3;
            def_value: 0;
            names: {
                0: string(STR_PARAM_LANDSCAPE_CLIMATE);
                1: string(STR_PARAM_LANDSCAPE_TEMPERATE);
                2: string(STR_PARAM_LANDSCAPE_ARCTIC);
                3: string(STR_PARAM_LANDSCAPE_SUBTROPIC);
            };
        }
    }

    param 2 {
        additional_colors {
            type: int;
            name: string(STR_PARAM_NAME_ADDITIONAL_COLORS);
            desc: string(STR_PARAM_DESC_ADDITIONAL_COLORS);
            min_value: 0;
            max_value: 2;
            def_value: 1;
            names: {
                0: string(STR_PARAM_ADDITIONAL_COLORS_NONE);
                1: string(STR_PARAM_ADDITIONAL_COLORS_SENSIBLE);
                2: string(STR_PARAM_ADDITIONAL_COLORS_RAINBOW);
            };
        }
    }

    param 3 {
        church_limit {
            type: int;
            name: string(STR_PARAM_NAME_CHURCH_LIMIT);
            min_value: 0;
            max_value: 2;
            def_value: 0;
            names: {
                0: string(STR_PARAM_CHURCH_LIMIT_ONE);
                1: string(STR_PARAM_CHURCH_LIMIT_EACH);
                2: string(STR_PARAM_CHURCH_LIMIT_LIMITLESS);
            };
        }
    }

    param 4 {
        stadium_limit {
            type: int;
            name: string(STR_PARAM_NAME_STADIUM_LIMIT);
            min_value: 0;
            max_value: 2;
            def_value: 0;
            names: {
                0: string(STR_PARAM_STADIUM_LIMIT_ONE);
                1: string(STR_PARAM_STADIUM_LIMIT_EACH);
                2: string(STR_PARAM_STADIUM_LIMIT_LIMITLESS);
            };
        }
    }

    param 5 {
        hotel_tourist {
            type: int;
            name: string(STR_PARAM_NAME_HOTEL_TOURIST);
            desc: string(STR_PARAM_DESC_HOTEL_TOURIST);
            min_value: 0;
            max_value: 3;
            def_value: 0;
            names: {
                0: string(STR_PARAM_HOTEL_TOURIST_NO);
                1: string(STR_PARAM_HOTEL_TOURIST_ACCEPT);
                2: string(STR_PARAM_HOTEL_TOURIST_PRODUCE_UNIQUE);
                3: string(STR_PARAM_HOTEL_TOURIST_PRODUCE);
            };
        }
    }
}

cargotable {
    PASS, MAIL, GOOD, FOOD, WATR, FZDR, SWET, TOUR, PETR
}

// shared functions

// hotel tourist production

switch (FEAT_HOUSES, SELF, hotel_tourist_production, getbits(extra_callback_info1, 0, 8)) {
    0: return cargotype("TOUR")*256 + 2;
    1: return cargotype("PASS")*256 + 2;
    2: return cargotype("MAIL")*256 + 1;
    return CB_RESULT_HOUSE_NO_MORE_PRODUCTION;
}

// folly castle tourist production

switch (FEAT_HOUSES, SELF, folly_castle_tourist_production, getbits(extra_callback_info1, 0, 8)) {
    0: return cargotype("TOUR")*256 + 4;
    1: return cargotype("PASS")*256 + 2;
    2: return cargotype("MAIL")*256 + 1;
    return CB_RESULT_HOUSE_NO_MORE_PRODUCTION;
}

