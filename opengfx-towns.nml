}

random_switch (FEAT_HOUSES, SELF, modest_shops_and_offices_3_color) {
    1: return PALETTE_USE_DEFAULT;
    1: return PALETTE_STRUCT_BLUE;
    1: return PALETTE_STRUCT_BROWN;
    1: return PALETTE_STRUCT_WHITE;
    1: return PALETTE_STRUCT_GREEN;
    1: return PALETTE_STRUCT_CONCRETE;
    1: return PALETTE_NEWSTRUCT_CRASH;
    1: return PALETTE_NEWSTRUCT_CREAM;
    1: return PALETTE_NEWSTRUCT_MAUVE;
    1: return PALETTE_NEWSTRUCT_BROWN;
}

if (enable_temperate || enable_uktownset) {
    item(FEAT_HOUSES, item_ukts_ogfx_offices_18) {
        property {
            substitute: 0x04;
            name: TTD_STR_TOWN_BUILDING_NAME_OFFICES_1;
            population: 72;
            mail_multiplier: 20;
            years_available: [0, 65535];
            accepted_cargos: [[PASS, 6], [MAIL, 4], [GOOD, 3]];
            availability_mask: [
                bitmask(TOWNZONE_INNER_SUBURB, TOWNZONE_CENTRE),
                bitmask(CLIMATE_TEMPERATE, CLIMATE_ARCTIC, CLIMATE_TROPIC, ABOVE_SNOWLINE)
            ];
        }
        if (additional_colors == 0)
        {
            graphics {
                construction_anim: switch_2_variants;
                default: callback_ukts_ogfx_offices_18;
                colour: shops_and_offices_3_color();
            }
        }
        if (additional_colors == 1)
        {
            graphics {
                construction_anim: switch_2_variants;
                default: callback_ukts_ogfx_offices_18;
                colour: modest_shops_and_offices_3_color();
            }
        }
        if (additional_colors == 2)
        {
            graphics {
                construction_anim: switch_2_variants;
                default: callback_ukts_ogfx_offices_18;
                colour: random_struct_color();
            }
        }
    }
}

