// null sprite

template sprite_null() { [0, 0, "gfx/null.png"]}

if (disable_base_houses) {
    disable_item(FEAT_HOUSES);
}

// building classes:
// 1 temperate church
// 2 temperate cathedral
// 3 arctic church
// 4 tropic church

// 10 soccer stadium
// 11 rugby stadium

// 20 college
// 21 school
// 22 warehouse (ukts)
// 23 tall office (ukts)
// 24 skyscraper (ukts)

// 30 park/statue/fountain

// 100 radio stuff (transmitter/radio house)
// 101 workshop (sawmill derivates)
// 102 university
// 103 courthouse

switch (FEAT_HOUSES, SELF, stadium_construction_check, (
    stadium_limit == 2
    || (stadium_limit == 1 && same_class_count_town == 0)
    || (stadium_limit == 0 && other_class_count_town(10) == 0
        && other_class_count_town(11) == 0)
)) {return;}

switch (FEAT_HOUSES, SELF, church_construction_check, (
    church_limit == 2
    || (church_limit == 1 && same_class_count_town == 0)
    || (church_limit == 0 && other_class_count_town(1) == 0
        && other_class_count_town(2) == 0
        && other_class_count_town(3) == 0
        && other_class_count_town(4) == 0)
)) {return;}

// population check for UKTS
switch (FEAT_HOUSES, PARENT, has_population, required, population >= required) {return;}

switch (FEAT_HOUSES, SELF, college_construction_check, (
    has_population((same_class_count_town + 1) * 3000)
    && !nearest_house_matching_criterion(12, SEARCH_HOUSE_BY_CLASS)
)) {return;}

switch (FEAT_HOUSES, SELF, switch_check_road_north_4_faced, nearby_tile_class(-1, 0)) {
    TILE_CLASS_ROAD:  4;
    1 + pseudo_random_bits;
}
switch (FEAT_HOUSES, SELF, switch_check_road_east_4_faced, nearby_tile_class(0, 1)) {
    TILE_CLASS_ROAD:  3;
    switch_check_road_north_4_faced;
}
switch (FEAT_HOUSES, SELF, switch_check_road_south_4_faced, nearby_tile_class(1, 0)) {
    TILE_CLASS_ROAD:  2;
    switch_check_road_east_4_faced;
}
switch (FEAT_HOUSES, SELF, switch_check_road_west_4_faced, nearby_tile_class(0, -1)) {
    TILE_CLASS_ROAD:  1;
    switch_check_road_south_4_faced;
}
switch (FEAT_HOUSES, SELF, switch_check_road_initial_4_faced, animation_frame) {
    0:  switch_check_road_west_4_faced;
    CB_RESULT_DO_NOTHING;
}

switch (FEAT_HOUSES, SELF, switch_check_road_north_2_faced, nearby_tile_class(-1, 0)) {
    TILE_CLASS_ROAD:  2;
    1 + (pseudo_random_bits & 0x01);
}
switch (FEAT_HOUSES, SELF, switch_check_road_east_2_faced, nearby_tile_class(0, 1)) {
    TILE_CLASS_ROAD:  1;
    switch_check_road_north_2_faced;
}
switch (FEAT_HOUSES, SELF, switch_check_road_south_2_faced, nearby_tile_class(1, 0)) {
    TILE_CLASS_ROAD:  2;
    switch_check_road_east_2_faced;
}
switch (FEAT_HOUSES, SELF, switch_check_road_west_2_faced, nearby_tile_class(0, -1)) {
    TILE_CLASS_ROAD:  1;
    switch_check_road_south_2_faced;
}
switch (FEAT_HOUSES, SELF, switch_check_road_initial_2_faced, animation_frame) {
    0:  switch_check_road_west_2_faced;
    CB_RESULT_DO_NOTHING;
}

switch (FEAT_HOUSES, SELF, radio_construction_check, (
    has_population((same_class_count_town + 1) * 2000)
    && !nearest_house_matching_criterion(12, SEARCH_HOUSE_BY_CLASS)
)) {return;}

switch (FEAT_HOUSES, PARENT, has_houses, required, num_houses >= required) {return;}

switch (FEAT_HOUSES, SELF, amenities_construction_check, (
    has_houses((same_class_count_town + 1) * 20)
    && !nearest_house_matching_criterion(3, SEARCH_HOUSE_BY_CLASS)
)) {return;}

switch (FEAT_HOUSES, SELF, park_construction_check, (
    amenities_construction_check() && terrain_type != TILETYPE_DESERT
)) {return;}

switch (FEAT_HOUSES, SELF, workshop_construction_check, (
    has_population((same_class_count_town + 1) * 650)
//    && !nearest_house_matching_criterion(7, SEARCH_HOUSE_BY_CLASS)
)) {return;}

switch (FEAT_HOUSES, SELF, ukts_skyscraper_construction_check, (
    has_population(5000)
    && has_population((same_class_count_town + 1) * 2500)
)) {return;}

switch (FEAT_HOUSES, SELF, ukts_tall_office_block_construction_check, (
    has_population(2000)
    && has_population((same_class_count_town + 1) * 1000)
)) {return;}

switch (FEAT_HOUSES, SELF, ukts_warehouse_construction_check, (
    has_population((same_class_count_town + 1) * 1000)
    && !nearest_house_matching_criterion(5, SEARCH_HOUSE_BY_CLASS)
)) {return;}

switch (FEAT_HOUSES, SELF, school_construction_check, (
    has_population((same_class_count_town + 1) * 1000)
    && !nearest_house_matching_criterion(12, SEARCH_HOUSE_BY_CLASS)
)) {return;}

